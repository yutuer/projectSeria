<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pureland.common.db.dao.mysql.mapper.ArmoryModelMapper">

    <select id="queryArmoryModelList" resultType="armoryModel">
        SELECT  `id` as id,
                `race_type` as raceType,
                `build_type` as buildType,
			    `base_id` as baseId
		FROM `armory_model`
		WHERE 1=1
    </select>

</mapper>



